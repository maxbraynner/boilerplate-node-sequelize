{
  "name": "boilerplate-node-sequelize",
  "scripts": {
    "sequelize": "./node_modules/.bin/sequelize",
    "db:start": "npm run sequelize db:migrate && npm run sequelize db:seed:all",
    "db:migrate": "npm run build && npm run sequelize db:migrate",
    "db:seed": "npm run build && npm run sequelize db:seed:all",
    "db:undo": "npm run build && npm run sequelize db:seed:undo:all && npm run sequelize db:migrate:undo:all",
    "db:reset": "npm run build && npm run db:undo && npm run db:start",
    "start": "npm run db:start && node lib/index.js",
    "start:debug": "node --inspect=0.0.0.0:9229 -r ts-node/register src/index.ts",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js",
    "lint": "tslint --project tsconfig.json",
    "build": "node node_modules/.bin/gulp default",
    "prune": "node yarn-prune.js",
    "postinstall": "test -n \"$NO_YARN_POSTINSTALL\" || npm run build",
    "test": "rm -rf ./test/reports && mocha --recursive --timeout 15000 -r ts-node/register test/app/**/*.ts --reporter mochawesome --reporter-options reportDir=./test/reports/,reportFilename=tests-result || exit 0"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=8.9.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run start:debug",
    "ext": "ts"
  },
  "dependencies": {
    "boom": "^7.2.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "~4.16.0",
    "express-validation": "^1.0.2",
    "firebase-admin": "^7.0.0",
    "helmet": "^3.12.1",
    "html-pdf": "^2.2.0",
    "joi": "^14.3.1",
    "joi-cpf-cnpj": "^0.1.0",
    "morgan": "~1.9.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "qrcode": "^1.2.0",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^5.4.0"
  },
  "devDependencies": {
    "@types/boom": "^7.2.0",
    "@types/chai": "^4.1.3",
    "@types/chai-http": "^3.0.4",
    "@types/cookie-parser": "^1.4.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.11.1",
    "@types/faker": "^4.1.2",
    "@types/helmet": "^0.0.42",
    "@types/http-status": "^0.2.30",
    "@types/joi": "^14.3.2",
    "@types/mocha": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/qrcode": "^1.2.0",
    "@types/request": "^2.47.0",
    "@types/request-promise-native": "^1.0.14",
    "@types/sequelize": "^4.27.20",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "faker": "^4.1.0",
    "firebase-mock": "^2.2.2",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^5.0.0",
    "http-status": "^1.1.0",
    "mocha": "^6.0.1",
    "mochawesome": "^3.0.2",
    "nodemon": "^1.17.5",
    "pg-promise": "^8.4.4",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.8.0",
    "typescript": "^3.3.3333"
  },
  "private": true
}
